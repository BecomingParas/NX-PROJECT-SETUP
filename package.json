{
  "name": "@nxproject/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
     "dev:backend": "npx nx run backend:serve",
    "dev:frontend": "npx nx run frontend:serve",
    "build:backend": "npx nx run backend:build",
    "build:frontend": "npx nx run frontend:build",
    "build": "yarn build:backend && yarn build:frontend",
    "db:generate": "yarn prisma generate --schema ./libs/database/src/prisma/schema.prisma",
    "db:migrate": "yarn prisma migrate dev --schema ./libs/database/src/prisma/schema.prisma",
    "db:push": "yarn prisma db push --schema ./libs/database/src/prisma/schema.prisma",
    "db:studio": "yarn prisma studio --schema ./libs/database/src/prisma/schema.prisma",
    "seed:team": "ts-node -r tsconfig-paths/register libs/database/src/seed/team.ts"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.6.0",
    "express": "~4.18.1",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "19.8.14",
    "@nx/eslint": "19.8.14",
    "@nx/eslint-plugin": "19.8.14",
    "@nx/jest": "19.8.14",
    "@nx/js": "19.8.14",
    "@nx/next": "19.8.14",
    "@nx/node": "19.8.14",
    "@nx/workspace": "19.8.14",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "15.0.6",
    "@types/express": "~4.17.13",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.35.0",
